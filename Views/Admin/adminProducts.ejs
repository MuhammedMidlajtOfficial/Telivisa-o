<script>


      
//  confirmation
    function deleteConfirmation(){
        return confirm("Are you sure?");
    }

// Block confirmation
    document.addEventListener("DOMContentLoaded", function () {
        const blockButton = document.querySelectorAll('.blockConfirmation');

        blockButton.forEach((elem)=>{
            elem.addEventListener('click',()=>{
                const queryValue = deleteConfirmation();
                const userId = elem.dataset.userid;

                if(queryValue){
                  const url = "/admin/adminBlockProduct?id="+userId+"&confirmation=" + encodeURIComponent(queryValue);
                  elem.href = url;
                }else{
                  console.log('Confirmation : false');
                }
            })
        })
    })

// Unblock confirmation
    document.addEventListener("DOMContentLoaded", function () {
        const blockButton = document.querySelectorAll('.unBlockConfirmation');

        blockButton.forEach((elem)=>{
            elem.addEventListener('click',()=>{
                const queryValue = deleteConfirmation();
                const userId = elem.dataset.userid;

                if(queryValue){
                  const url = "/admin/adminUnblockProduct?id="+userId+"&confirmation=" + encodeURIComponent(queryValue);
                  elem.href = url;
                }else{
                  console.log('Confirmation : false');
                }
            })
        })
    })


</script>

