            <div class="card mb-4">
                <div class="card-body">
                    <table class="table table-hover" id="product">
                        <thead>
                            <tr>
                                <th>Product image</th>
                                <th>Product name</th>
                                <th>Price</th>
                                <th>Stock</th>
                                <th>Hoarding</th>
                                <th>Status</th>
                                <th class="text-end">Hoarding Action</th>
                            </tr>
                        </thead>
                        <tbody>

                            <% product.forEach((product)=>{ %>

                                <tr>
                                    <td><img src="<%= product.imageUrl[0].path %>" class="img-sm img-thumbnail" alt="product"></td>
                                    <td class="mb-2"><h6><%= product.productName %></h6></td>
                                    <td class="mb-2"><b> ₹ <%= product.productPrice %></b></td>
                                    <td class="mb-2"><b><%= product.productStock %></b></td>
                                    <% if(product.productHoarding === "Active") {%>
                                        <td class="mb-2"><b><span class="badge rounded-pill alert-danger"><%= product.productHoarding %></span></b></td>
                                    <% } else {%>
                                        <td class="mb-2"><b><span class="badge rounded-pill alert-success"><%= product.productHoarding %></span></b></td>
                                    <% } %>
                                    <% if(product.productStatus === "block") {%>
                                        <td class="mb-2"><b><span class="badge rounded-pill alert-danger"><%= product.productStatus %></span></b></td>
                                    <% } else {%>
                                        <td class="mb-2"><b><span class="badge rounded-pill alert-success"><%= product.productStatus %></span></b></td>
                                    <% } %>
                                    <td style="display: flex; justify-content: flex-end;">
                                        <% if(product.productHoarding === "Inactive") {%>
                                            <button class="btn btn-danger col-lg-6" style="width: 70px; align-items: center; display: flex; justify-content: center;">
                                                <a data-userid="<%= product._id %>" class="blockConfirmation" style="text-decoration: none; color: aliceblue;">Active</a>
                                            </button>
                                        <% } else if(product.productHoarding === "Active") { %>
                                            <button class="btn btn-success col-lg-6" style="width: 70px; align-items: center; display: flex; justify-content: center;">
                                                <a data-userid="<%= product._id %>" class="unBlockConfirmation" style="text-decoration: none; color: aliceblue;">Inactive</a>
                                            </button>
                                        <% } %>
                                    </td>
                                </tr>
                            <% }) %>
                            
