<body>
    <main class="main" style="height: 100vh;  display: flex; align-items: center; justify-content: center;">
        <section class="">
            <div class="container">
                <div style="width: 450px; background-color: white;">
                    <div class="login_wrap widget-taber-content p-30 background-white border-radius-10 mb-md-5 mb-lg-0 mb-sm-5">
                        <div class="padding_eight_all bg-white">
                            <div class="heading_s1">
                                <h3 class="mb-30">Admin Login</h3>
                            </div>
                            <% if( locals.emailDoesNotExist ){ %>
                                <div class="alert alert-danger">Check your email or password</div>
                            <% } %>
                            <% if( locals.adminLogout ){ %>
                                <div class="alert alert-success">Logout successfull</div>
                            <% } %>
                            <form method="post" action="/admin/postAdminLogin">
                                <div class="form-group">
                                    <input type="email" id="ALEmail" name="ALEmail" placeholder="Your Email" required="true">
                                </div>
                                <div class="form-group">
                                    <input type="password" id="ALPassword" name="ALPassword" placeholder="Password" required="true">
                                </div>
                                <div class="form-group">
                                    <button type="submit" class="btn btn-fill-out btn-block hover-up" name="login">Log in</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </main>
</main>


